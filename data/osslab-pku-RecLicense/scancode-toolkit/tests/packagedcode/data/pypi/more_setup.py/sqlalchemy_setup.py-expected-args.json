{
  "name": "SQLAlchemy",
  "description": "Database Abstraction Library",
  "author": "Mike Bayer",
  "author_email": "mike_mp@zzzcomputing.com",
  "url": "http://www.sqlalchemy.org",
  "package_dir": {
    "": "lib"
  },
  "license": "MIT License",
